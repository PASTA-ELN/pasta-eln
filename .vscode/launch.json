      "request": "launch",
      "console": "integratedTerminal"
    },
    {
      "name": "Pytest 10-20",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": [
        "-s",
        "tests/test_01_DefaultExample.py"
      ],
      "console": "integratedTerminal"
